WITH Cte AS (
    SELECT s.buyer_id, p.product_name
    FROM Sales s
    INNER JOIN Product p
    ON p.product_id = s.product_id
)

SELECT DISTINCT Cte.buyer_id
FROM Cte
WHERE Cte.product_name = 'S8'
AND Cte.buyer_id NOT IN (
    SELECT c.buyer_id
    FROM Cte c
    WHERE c.product_name = 'iPhone'
) ;
